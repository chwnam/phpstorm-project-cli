{
  "name": "changwoo/phpstorm-project-cli",
  "description": "Automate PhpStorm Workspace Settings",
  "autoload": {
    "psr-4": {
      "Changwoo\\PhpStormProjectCli\\": "src/"
    }
  },
  "require": {
    "php": "^8.1",
    "splitbrain/php-cli": "^1.3",
    "ext-dom": "*",
    "ext-mbstring": "*",
    "ext-simplexml": "*",
    "servo/fluidxml": "^2.0",
    "ulrichsg/getopt-php": "^4.0"
  },
  "require-dev": {
    "bamarni/composer-bin-plugin": "^1.8"
  },
  "config": {
    "allow-plugins": {
      "bamarni/composer-bin-plugin": true
    }
  }
}
